.checkin-select
  label(translate) Likelihood
  .dropdown
    button.btn.btn-default.dropdown-toggle(
      type='button',
      data-toggle='dropdown',
      aria-haspopup='true',
      aria-expanded='false')
      span {{selected.title}}
      span.caret
    ul.dropdown-menu
      li(ng-repeat='(key, value) in options')
        a(ng-click='select(key)') {{value.title}}